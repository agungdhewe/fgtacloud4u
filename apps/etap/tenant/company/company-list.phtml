<div id="pnl_list">
	<div class="fgta-page-title">Daftar</div>

	<div id="pnl_list-pnl_head" style="display: flex; align-items: flex-end">
		<!-- Simple Query -->
		<div class="list-buttonbaru" style="text-align: right; width: calc(100% - 10px); float: right;  position: absolute">
			<a href="javascript:void(0)" id="pnl_list-btn_new" class="easyui-linkbutton c8" style="width: 60px">Baru</a>
		</div>
		<div class="list-search-wrap" style="width: calc(100% - 65px); display: flex;">
			<div class="list-search-item" style="width: 50px;">Cari</div>
			<div class="list-search-item">
				<input id="pnl_list-txt_search" class="easyui-textbox" style="width: 100%">
			</div>
			<div class="list-search-item" style="width: 45px">
				<a href="javascript:void(0)" id="pnl_list-btn_load" class="easyui-linkbutton c8" style="width: 45px">Load</a>
			</div>
		</div>
	</div>


	<div style="margin-top: 10px">
		<table id="pnl_list-tbl_list" paging="true" cellspacing="0" width="100%">
			<thead>
				<tr>
					<th mapping="company_id">company_id</th>
					<th mapping="company_name">company_name</th>
					<th mapping="company_address">company_address</th>
					<th mapping="company_city">company_city</th>
					<th mapping="company_telp">company_telp</th>
					<th mapping="company_email">company_email</th>
					<th mapping="company_npwp">company_npwp</th>
					<th mapping="company_isdisabled">company_isdisabled</th>

				</tr>
				<tr style="background-color: #cccccc; height: 30px">
					<td class="fgtable-head" style="width: 100px; border-bottom: 1px solid #000000">company_id</td>
					<td class="fgtable-head" style="width: 100px; border-bottom: 1px solid #000000">company_name</td>
					<td class="fgtable-head" style="width: 100px; border-bottom: 1px solid #000000">company_address</td>
					<td class="fgtable-head" style="width: 100px; border-bottom: 1px solid #000000">company_city</td>
					<td class="fgtable-head" style="width: 100px; border-bottom: 1px solid #000000">company_telp</td>
					<td class="fgtable-head" style="width: 100px; border-bottom: 1px solid #000000">company_email</td>
					<td class="fgtable-head" style="width: 100px; border-bottom: 1px solid #000000">company_npwp</td>
					<td class="fgtable-head" style="width: 100px; border-bottom: 1px solid #000000">company_isdisabled</td>

				</tr>
			</thead>
		</table>
	</div>



</div>